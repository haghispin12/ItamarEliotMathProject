        android:label="calcultator"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calcultator"
        tools:targetApi="31">
        <activity
            android:name=".ai.chat"
            android:exported="false" />
        <activity
            android:name=".ai.MainActivity_ai"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mathproject.ShowFruitsActivity"
            android:exported="false" />
        <activity
            android:name=".mathproject.Rate_Activity"
            android:exported="false" />
        <activity
            android:name=".mathproject.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".mathproject.MainActivity"
            android:exported="false" />
    </application>

</manifest>